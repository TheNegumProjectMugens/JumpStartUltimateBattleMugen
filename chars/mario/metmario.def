; Definition file for Metal Mario
; Contains all the filenames needed for the character

; Player information
[Info]
name = "NGPC Metal Mario"		;Name of character
displayname = "Metal Mario"		;Name of character to display
versiondate = 2005.4.16			;Version date of character
mugenversion = 04,14,2002		;Version of M.U.G.E.N character works on
author = "N64Mario, TMasta & Soliduma"	;Character author name
pal.defaults = 1,2,3,4,5,6		;Default palettes in order of preference

; Files for the player
[Files]
cmd     = command.cmd  ;Command set
cns     = constant.cns ;Constants
stcommon = common1.cns ;Common states
st      = state.cns    ;General States
st1     = state-s.cns  ;Special Moves
st2     = state-h.cns  ;Super Moves
st3     = override.cns ;Override States
sprite  = metmario.sff ;Sprite
anim    = animation.air;Animation
sound   = sound.snd    ;Sound
pal1    = colors/metmario1.act    ;Palettes (can have up to 12)
pal2    = colors/metmario2.act
pal3    = colors/metmario3.act
pal4    = colors/ngpc-metmario1.act
pal5    = colors/ngpc-metmario2.act
pal6    = colors/ngpc-metmario3.act
